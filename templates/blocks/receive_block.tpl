<fieldset>
    <legend>To</legend>
    <div class="control-group">
        <label class="control-label" for="receiveWallet">Receiver Wallet Id</label>

        <div class="controls">
            <input type="text" name="receive[Wallet]" id="receiveWallet" placeholder="Receiver Wallet">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receiveName">Receiver Name</label>

        <div class="controls">
            <input type="text" name="receive[Name]" id="receiveName" placeholder="Receiver Name">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receiveAddress">Receiver Address</label>

        <div class="controls">
            <input type="text" name="receive[Address]" id="receiveAddress" placeholder="Receiver Address">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receiveCity">Receiver City</label>

        <div class="controls">
            <input type="text" name="receive[City]" id="receiveCity" placeholder="Receiver City">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receiveState">Receiver State</label>

        <div class="controls">
            <input type="text" name="receive[State]" id="receiveState" placeholder="Receiver State">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receivePostal">Receiver Zip/Postal Code</label>

        <div class="controls">
            <input type="text" name="receive[Postal]" id="receivePostal" placeholder="Receiver Zip/Postal Code" maxlength="5">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="receiveCounty">Receiver County</label>

        <div class="controls">
            <input type="text" name="receive[Country]" id="receiveCounty" placeholder="Receiver County">
        </div>
    </div>
</fieldset>
